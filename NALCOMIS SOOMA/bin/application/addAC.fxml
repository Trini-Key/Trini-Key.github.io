<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="addACWindow" alignment="CENTER" minHeight="400.0" minWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddWindowController">
   <children>
      <Region minWidth="100.0" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
      <Label text="Buno" />
      <TextField fx:id="addBuno" alignment="CENTER" maxWidth="400.0" promptText="i.e. 192461" />
      <Label text="Modex" />
      <TextField fx:id="addModex" alignment="CENTER" maxWidth="400.0" promptText="i.e. 00" />
      <Label text="Org" />
      <TextField fx:id="addTsn" alignment="CENTER" maxWidth="400.0" promptText="i.e. HMH-461" />
      <Label text="Type" />
      <TextField fx:id="addType" alignment="CENTER" maxWidth="400.0" promptText="i.e. CH-53E" />
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="addWinButton" mnemonicParsing="false" text="Add AC">
               <HBox.margin>
                  <Insets bottom="5.0" right="5.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
      <Region minWidth="100.0" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS" />
   </children>
   <effect>
      <DropShadow />
   </effect>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</VBox>
